{
  "name": "rs-rest-service-starter",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "types": "index.d.ts",
  "description": "REST service starter for RS School",
  "main": "src/server.ts",
  "scripts": {
    "start": "nodemon -L src/server.ts",
    "dev": "npx ts-node src/server.ts",
    "lint": "eslint --ext .js,.ts --fix src/",
    "test": "cross-env DEBUG=rs:* jest --testMatch \"<rootDir>/test/e2e/test/*.test.js\" --noStackTrace --runInBand",
    "test:auth": "cross-env DEBUG=rs:* TEST_MODE=auth jest --noStackTrace"
  },
  "keywords": [
    "rs",
    "school",
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0 <17"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "fastify": "^3.24.1",
    "fastify-swagger": "^4.12.6",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2",
    "winston": "^3.3.3",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@types/uuid": "^8.3.3",
    "fastify-plugin": "^3.0.0", 
    "lint-staged": "12.0.3",
    "nodemon": "2.0.15",
    "supertest": "6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
